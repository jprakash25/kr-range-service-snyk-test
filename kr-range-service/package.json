{
  "name": "kr-range-service",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "dependencies": {
    "@azure/msal-node": "^1.3.1",
    "@hapi/joi-date": "^2.0.1",
    "aws-sdk": "^2.828.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "cors": "^2.8.5",
    "csv-parse": "^4.16.0",
    "express": "^4.17.1",
    "express-oas-generator": "^1.0.30",
    "fast-csv": "^4.3.6",
    "highland": "^2.13.5",
    "https-proxy-agent": "^5.0.0",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.14",
    "moment-timezone": "^0.5.33",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-azure-ad": "^4.3.0",
    "pg": "^8.5.1",
    "pg-query-stream": "^4.0.0",
    "querystring": "^0.2.1",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "socket.io": "^4.0.2",
    "sqs-consumer": "^5.5.0",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "node ./index.js",
    "start:dev": "env-cmd -f .env.default yarn db:migrate && env-cmd -f .env.default nodemon ./index.js",
    "start:dev:docker": "yarn db:migrate && nodemon ./index.js",
    "build:services": "env-cmd -f .env.default docker-compose build",
    "localstack:setup": "env-cmd -f .env.default node ./local_stack/localstack-setup.js",
    "start:all:local": "env-cmd -f .env.default docker-compose up -d && sleep 10 && yarn localstack:setup",
    "lint": "eslint ./",
    "stop:all": "docker-compose down ",
    "test:unit": "env-cmd -f .env.default ./bin/testing unit",
    "test:integration": "env-cmd -f .env.default ./bin/testing integration",
    "test": "env-cmd -f .env.default ./bin/testing all",
    "test:report": "env-cmd -f .env.default ./bin/testing report",
    "db:migrate": "npx sequelize db:migrate --migrations-path db/migrations"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.22.0",
    "husky": "4",
    "istanbul": "^0.4.5",
    "istanbul-cobertura-badger": "^1.3.1",
    "jest": "^27.1.0",
    "lint-staged": "^10.5.4",
    "mockdate": "^3.0.2",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "stream-mock": "^2.0.5",
    "supertest": "^6.1.3"
  },
  "test": {
    "coverage": {
      "dir": "coverage",
      "unit": {
        "dir": "coverage/coverage_unit",
        "out": "coverage/coverage_unit/coverage-final.json"
      },
      "integration": {
        "dir": "coverage/coverage_integration",
        "out": "coverage/coverage_integration/coverage-final.json"
      },
      "combined": {
        "dir": "coverage/combined",
        "out": "coverage/combined/out.json"
      }
    }
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "test"
    ],
    "ext": "js",
    "ignore": [
      "*.test.js"
    ],
    "delay": "3"
  },
  "engines": {
    "node": "14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo [Husky] pre-commit && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint --fix"
    ]
  }
}
